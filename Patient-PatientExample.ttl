  fhir:nodeRole fhir:treeRoot ;
  fhir:id [ fhir:v "PatientExample"] ; # 
  fhir:meta [
     fhir:profile ( [
       fhir:v "https://nih-ncpi.github.io/ncpi-fhir-ig-2/StructureDefinition/MyPatient"^^xsd:anyURI ;
